{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowJs": true,
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "paths": {
      "packages/*": ["./packages/my-ui-owntest/*"]
    }
  },
  "include": [
    "packages/my-ui-owntest/**/*.ts",
    "packages/my-ui-owntest/**/*.d.ts",
    "packages/my-ui-owntest/**/*.tsx",
    "packages/my-ui-owntest/**/*.vue"
  ]
}
